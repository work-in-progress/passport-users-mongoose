{
  "author": "Martin Wawrusch <martin@wawrusch.com> (http://martinatsunset.com)",
  "name": "passport-users-mongoose",
  "description": "Provides a canonical mongodb user and authentication store as part of the passport (https://github.com/jaredhanson/passport) family.",
  "version": "0.0.1",
  "main" : "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/scottyapp/passport-users-mongoose.git"
  },
  "bin" : {
  },
  "directories" : {
    "lib" : "./lib",
    "test" : "./spec"
  },
  "keywords" : [
    "mongoose","passport","auth"
  ],
  "scripts": {
    "test": "node_modules/.bin/vows --spec spec/*-spec.coffee",
    "watch" : "coffee -c -w  -o lib src"
  },
  "engines": {
    "node": ">=0.4.8"
  },
  "dependencies": {
    "mongoose": "2.3.x",
    "mongoose-types" : "1.0.x",
    "lingo": "0.0.x",
    "pkginfo" : "~ 0.2",
    "bcrypt" : "0.4.x"    
  },
  "devDependencies": {
    "underscore" : "1.2.x",
    "async" : "0.1.x",
    "colors" : "~ 0.5",
    "should" : ">=0.2.1",    
    "coffee-script" : ">= 1.1.2",
    "vows" : ">= 0.5.9",
    "express" : "2.4.x",
    "gleak" : ">=0.2.1",
    "database-cleaner" : ">= 0.5.0"
  },
  "bugs": { "url": "http://github.com/scottyapp/passport-users-mongoose/issues" },
  "licenses": [
    { "type": "MIT",
      "url": "http://github.com/scottyapp/passport-users-mongoose/raw/master/LICENSE"
    }
  ]
}
